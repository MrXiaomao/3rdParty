/* QLineEdit基础样式 */
QLineEdit{
  color:palette(text); /* 文字颜色使用系统文本色 */
}

/* 空文本状态下的QLineEdit样式 */
QLineEdit[text=""]{ 
  color:palette(text); /* 空文本时文字颜色仍使用系统文本色（默认可能为灰色提示文字） */
}

/* 行文本编辑框只读属性样式 */
QLineEdit:read-only{
  background:palette(text); /* 文字颜色使用系统文本色 */
}

/* 行文本编辑框禁用属性样式 */
QLineEdit:disabled{
  background:palette(window); /* 文字颜色使用系统文本色 */
}

/* QTextEdit基础样式 */
QTextEdit{
  color:palette(text); /* 文字颜色使用系统文本色 */
  border:0px solid palette(highlight); /* 边框：2px系统高亮色实线 */
  border-radius:0px; /* 边框无圆角（直角） */
}

/* 多行富文本编辑框只读属性样式 */
QTextEdit:read-only{
  background:palette(text); /* 文字颜色使用系统文本色 */
}

/* 多行富文本编辑框禁用属性样式 */
QTextEdit:disabled{
  background:palette(window); /* 文字颜色使用系统文本色 */
}

/* 空文本状态下的QTextEdit样式 */
QTextEdit[text=""]{ 
  color:palette(text); /* 空文本时文字颜色仍使用系统文本色 */
  border:0px solid palette(highlight); /* 保持边框样式不变 */
  border-radius:0px; /* 保持直角边框 */
}

/* QPlainTextEdit基础样式 */
QPlainTextEdit{
  color:palette(text); /* 文字颜色使用系统文本色 */
}

/* 空文本状态下的QPlainTextEdit样式 */
QPlainTextEdit[text=""]{ 
  color:palette(text); /* 空文本时文字颜色仍使用系统文本色 */
}

/* 工具提示（QToolTip）样式 */
QToolTip{
  color:palette(text); /* 文字颜色使用系统文本色 */
  background-color:palette(base); /* 背景色使用系统基础色 */
  border:1px solid palette(highlight); /* 边框：1px系统高亮色实线 */
  border-radius:0px; /* 边框无圆角 */
}

/* 状态栏（QStatusBar）样式 */
QStatusBar{
  background-color:qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0 rgba(25,25,25,127),stop:1 rgba(53,53,53,75)); /* 背景：垂直渐变灰色（上深下浅） */
  color:palette(text); /* 文字颜色使用系统文本色 */
}

/* 状态栏子项中的QLabel样式 */
QStatusBar::item QLabel {
  font-size: 16px; /* 字体大小设为16px */
}

/* 菜单栏（QMenuBar）基础样式 */
QMenuBar{
  background-color:transparent; /* 背景透明 */
  spacing:0px; /* 菜单项之间的间距设为0px */
}

/* 菜单栏项（QMenuBar::item）基础样式 */
QMenuBar::item{
  spacing:2px; /* 菜单项内部元素间距设为2px */
  padding:0px 4px; /* 内边距：上下0px，左右4px */
  background:transparent; /* 背景透明 */
}

/* 菜单栏项选中状态 */
QMenuBar::item:selected{
  background-color:rgba(80,80,80,255); /* 背景色：深灰色不透明 */
}

/* 工具按钮（QToolButton）悬停状态 */
QToolButton:hover{
  background-color:rgba(80,80,80,255); /* 悬停时背景色：深灰色不透明 */
}

/* 菜单栏项按下状态样式 */
QMenuBar::item:pressed{
  background-color:palette(highlight); /* 背景色使用系统高亮色 */
  color:rgba(255,255,255,255); /* 文字颜色为纯白色 */
  border-left:1px solid rgba(25,25,25,127); /* 左侧边框：1px深灰色半透明 */
  border-right:1px solid rgba(25,25,25,127); /* 右侧边框：同上 */
}

/* 菜单整体样式 */
QMenu{
  background-color:palette(window); /* 背景色使用系统窗口色 */
  border:1px solid palette(shadow); /* 边框使用系统阴影色 */
}

/* 菜单项基础样式 */
QMenu::item{
  padding:3px 15px 3px 15px; /* 内边距：上下3px，左右15px */
  border:1px solid transparent; /* 透明边框，为选中状态预留空间 */
}

/* 菜单项禁用状态 */
QMenu::item:disabled{
  background-color:rgba(35,35,35,127); /* 背景色：深灰色半透明 */
  color:palette(disabled); /* 文字颜色使用系统禁用色 */
}

/* 菜单项选中状态 */
QMenu::item:selected{
  border-color:rgba(147,191,236,127); /* 边框颜色：浅蓝色半透明 */
  background:palette(highlight); /* 背景色使用系统高亮色 */
  color:rgba(255,255,255,255); /* 文字颜色为纯白色 */
}

/* 菜单项图标左边距 */
QMenu::icon {
  padding-left: 10px; /* 图标左内边距设为10px（默认通常为0） */
}

/* 选中状态的菜单项图标样式 */
QMenu::icon:checked{
  background-color:qlineargradient(x1:0,y1:1,x2:0,y2:0,stop:0 rgba(25,25,25,127),stop:1 rgba(53,53,53,75)); /* 背景：从上到下的渐变灰色 */
  border:1px solid palette(highlight); /* 边框使用系统高亮色 */
  border-radius:2px; /* 圆角：2px */
}

/* 菜单分隔线样式 */
QMenu::separator{
  height:1px; /* 高度：1px */
  background:palette(alternate-base); /* 背景色使用系统交替基础色 */
  margin-left:2px; /* 左外边距：2px */
  margin-right:2px; /* 右外边距：2px */
}

/* 菜单项指示器（如复选框、单选框）基础样式 */
QMenu::indicator{
  width:15px; /* 宽度：15px */
  height:15px; /* 高度：15px */
}

/* 非排他性指示器（复选框）选中状态 */
QMenu::indicator:non-exclusive:checked{
  padding-left:2px; /* 左内边距：2px（用于调整位置） */
}

/* 非排他性指示器（复选框）未选中状态 */
QMenu::indicator:non-exclusive:unchecked{
  padding-left:2px; /* 同上 */
}

/* 排他性指示器（单选框）选中状态 */
QMenu::indicator:exclusive:checked{
  padding-left:2px; /* 同上 */
}

/* 排他性指示器（单选框）未选中状态 */
QMenu::indicator:exclusive:unchecked{
  padding-left:2px; /* 同上 */
}

/* 顶部工具栏样式 */
QToolBar::top{
  background-color:qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0 rgba(25,25,25,127),stop:1 rgba(53,53,53,75)); /* 背景：垂直渐变灰色（上深下浅） */
  border-bottom:3px solid qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0 rgba(25,25,25,127),stop:1 rgba(53,53,53,75)); /* 底部边框：3px渐变灰色 */
}

/* 底部工具栏样式 */
QToolBar::bottom{
  background-color:qlineargradient(x1:0,y1:1,x2:0,y2:0,stop:0 rgba(25,25,25,127),stop:1 rgba(53,53,53,75)); /* 背景：垂直渐变灰色（下深上浅） */
  border-top:3px solid qlineargradient(x1:0,y1:1,x2:0,y2:0,stop:0 rgba(25,25,25,127),stop:1 rgba(53,53,53,75)); /* 顶部边框：3px渐变灰色 */
}

/* 左侧工具栏样式 */
QToolBar::left{
  background-color:qlineargradient(x1:0,y1:0,x2:1,y2:0,stop:0 rgba(25,25,25,127),stop:1 rgba(53,53,53,75)); /* 背景：水平渐变灰色（左深右浅） */
  border-right:3px solid qlineargradient(x1:0,y1:0,x2:1,y2:0,stop:0 rgba(25,25,25,127),stop:1 rgba(53,53,53,75)); /* 右侧边框：3px渐变灰色 */
}

/* 右侧工具栏样式 */
QToolBar::right{
  background-color:qlineargradient(x1:1,y1:0,x2:0,y2:0,stop:0 rgba(25,25,25,127),stop:1 rgba(53,53,53,75)); /* 背景：水平渐变灰色（右深左浅） */
  border-left:3px solid qlineargradient(x1:1,y1:0,x2:0,y2:0,stop:0 rgba(25,25,25,127),stop:1 rgba(53,53,53,75)); /* 左侧边框：3px渐变灰色 */
}

/* 主窗口分隔器样式 */
QMainWindow::separator{
  width:6px; /* 水平分隔器宽度 */
  height:5px; /* 垂直分隔器高度 */
  padding:2px; /* 内边距：2px */
}

/* 组合框基础样式 */
QComboBox {
  combobox-popup: 0; /* 禁用默认下拉弹窗样式（使用自定义视图） */
}

/* 组合框下拉列表视图样式 */
QComboBox QAbstractItemView {
  border: 1px solid palette(shadow); /* 边框：1px系统阴影色实线 */
}

/* 组合框列表项选中状态 */
QComboBox:item:selected {
  border: none; /* 无选中边框 */
  background:palette(highlight); /* 背景色：系统高亮色 */
  color:rgba(255,255,255,255); /* 文字颜色：白色 */
}

/* 组合框指示器样式 */
QComboBox::indicator{
  background-color:transparent; /* 背景透明 */
  selection-background-color:transparent; /* 选中时背景透明 */
  color:transparent; /* 文字透明 */
  selection-color:transparent; /* 选中时文字透明 */
}

/* 水平分割器手柄样式 */
QSplitter::handle:horizontal{
  background:palette(base); /* 背景色：系统基础色 */
  width:10px; /* 手柄宽度 */
}

/* 垂直分割器手柄样式 */
QSplitter::handle:vertical{
  background:palette(base); /* 背景色：系统基础色 */
  height:10px; /* 手柄高度 */
}

/* 分隔器和分割器手柄悬停状态 */
QMainWindow::separator:hover,QSplitter::handle:hover{
  background:palette(highlight); /* 悬停时背景色：系统高亮色 */
}

/* 停靠窗口标题栏样式 */
QDockWidget::title{
  padding:4px; /* 内边距：4px */
  background-color:qlineargradient(x1:0,y1:1,x2:0,y2:0,stop:0 rgba(25,25,25,127),stop:1 rgba(53,53,53,75)); /* 背景：垂直渐变灰色（下深上浅） */
  border:1px solid rgba(25,25,25,75); /* 边框：1px半透明深灰色 */
  border-bottom:2px solid rgba(25,25,25,75); /* 底部边框：2px半透明深灰色 */
}

/* 停靠窗口基础样式 */
QDockWidget{
  titlebar-close-icon:url(":/qss_icons/dark/dark_icons/window_close.png"); /* 关闭按钮图标 */
  titlebar-normal-icon:url(":/qss_icons/dark/dark_icons/window_undock.png"); /* 浮动按钮图标 */
}

/* 停靠窗口关闭和浮动按钮位置 */
QDockWidget::close-button,QDockWidget::float-button{
  subcontrol-position:top right; /* 子控件位置：右上角 */
  subcontrol-origin:margin; /* 子控件原点：边距 */
  position:absolute; /* 绝对定位 */
  top:3px; /* 顶部距离 */
  bottom:0px; /* 底部距离 */
  width:20px; /* 宽度 */
  height:20px; /* 高度 */
}

/* 关闭按钮位置 */
QDockWidget::close-button{
  right:3px; /* 右侧距离 */
}

/* 浮动按钮位置 */
QDockWidget::float-button{
  right:25px; /* 右侧距离（关闭按钮右侧22px） */
}

/* 分组框基础样式 */
QGroupBox{
  background-color:rgba(66,66,66,50%); /* 背景：半透明深灰色 */
  margin-top:27px; /* 顶部外边距：27px（为标题预留空间） */
  border:1px solid rgba(25,25,25,127); /* 边框：1px半透明深灰色 */
  border-radius:4px; /* 圆角：4px */
}

/* 分组框标题样式 */
QGroupBox::title{
  subcontrol-origin:margin; /* 子控件原点：边距 */
  subcontrol-position:left top; /* 子控件位置：左上角 */
  padding:4px 6px; /* 内边距：上下4px，左右6px */
  margin-left:3px; /* 左侧外边距：3px */
  background-color:qlineargradient(x1:0,y1:1,x2:0,y2:0,stop:0 rgba(25,25,25,127),stop:1 rgba(53,53,53,75)); /* 背景：垂直渐变灰色（下深上浅） */
  border:1px solid rgba(25,25,25,75); /* 边框：1px半透明深灰色 */
  border-bottom:2px solid rgb(127,127,127); /* 底部边框：2px中灰色 */
  border-top-left-radius:4px; /* 左上角圆角：4px */
  border-top-right-radius:4px; /* 右上角圆角：4px */
}

/* 标签页容器样式 */
QTabWidget::pane{
  background-color:palette(disabled); /* 背景色：系统禁用色 */
  border-top:1px solid rgba(25,25,25,50%); /* 顶部边框：1px半透明深灰色 */
}

/* 标签栏布局 */
QTabWidget::tab-bar{
  alignment: left; /* 对齐方式：左对齐 */
  left:3px; /* 左侧距离：3px */
  top:1px; /* 顶部距离：1px */
}

/* 标签栏基础样式 */
QTabBar{
  background-color:transparent; /* 背景透明 */
  qproperty-drawBase:0; /* 禁用默认背景绘制 */
  border-bottom:1px solid rgba(25,25,25,50%); /* 底部边框：1px半透明深灰色 */
}

/* 标签页基础样式 */
QTabBar::tab{
  padding:4px 6px; /* 内边距：上下4px，左右6px */
  background-color:transparent; /* 背景透明 */
  background-color:qlineargradient(x1:0,y1:1,x2:0,y2:0,stop:0 rgba(25,25,25,127),stop:1 rgba(53,53,53,75)); /* 背景：垂直渐变灰色（下深上浅） */
  border:1px solid rgba(25,25,25,75); /* 边框：1px半透明深灰色 */
  border-top-left-radius:4px; /* 左上角圆角：4px */
  border-top-right-radius:4px; /* 右上角圆角：4px */
}

/* 标签页选中/悬停状态 */
QTabBar::tab:selected,QTabBar::tab:hover{
  background-color:transparent; /* 背景透明 */
  background-color:qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0 rgba(53,53,53,127),stop:1 rgba(66,66,66,50%)); /* 背景：垂直渐变灰色（上深下浅） */
  border-bottom-color:rgba(66,66,66,75%); /* 底部边框色：半透明深灰色 */
}

/* 标签页选中状态 */
QTabBar::tab:selected{
  border-bottom:2px solid palette(highlight); /* 底部边框：2px系统高亮色 */
}

/* 标签页选中禁用状态 */
QTabBar::tab::selected:disabled{
  border-bottom:2px solid rgb(127,127,127); /* 底部边框：2px中灰色 */
}

/* 标签页关闭按钮基础样式 */
QTabBar::close-button {
  border:1px solid transparent; /* 边框：1px透明 */
  border-radius:2px; /* 圆角：2px */
  image: url(":/qss_icons/dark/dark_icons/window_close.png"); /* 关闭图标 */
}

/* 关闭按钮悬停状态 */
QTabBar::close-button:hover {
  background-color:qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0 rgba(106,106,106,255),stop:1 rgba(106,106,106,75)); /* 背景：垂直渐变浅灰色 */
  border:1px solid palette(base); /* 边框：1px系统基础色 */
  image: url(":/qss_icons/dark/dark_icons/window_close_focus.png"); /* 悬停关闭图标 */
}

/* 关闭按钮按下状态 */
QTabBar::close-button:pressed {
  background-color:qlineargradient(x1:0,y1:1,x2:0,y2:0,stop:0 rgba(25,25,25,127),stop:1 rgba(53,53,53,75)); /* 背景：垂直渐变深灰色 */
  border:1px solid palette(base); /* 边框：1px系统基础色 */
  image: url(":/qss_icons/dark/dark_icons/window_close_pressed.png"); /* 按下关闭图标 */
}

/* 复选框指示器尺寸 */
QCheckBox::indicator{
  width:18px; /* 宽度 */
  height:18px; /* 高度 */
}

/* 单选按钮指示器尺寸 */
QRadioButton::indicator{
  width:18px; /* 宽度 */
  height:18px; /* 高度 */
}

/* 树视图和表格视图基础样式 */
QTreeView, QTableView{
  alternate-background-color:palette(window); /* 交替行背景色：系统窗口色 */
  background:palette(base); /* 背景色：系统基础色 */
}

/* 表头样式 */
QTreeView QHeaderView::section, QTableView QHeaderView::section{
  background-color:qlineargradient(x1:0,y1:1,x2:0,y2:0,stop:0 rgba(25,25,25,127),stop:1 rgba(53,53,53,75)); /* 背景：垂直渐变灰色（下深上浅） */
  border-style:none; /* 无边框 */
  border-bottom:1px solid palette(dark); /* 底部边框：1px系统暗色 */
  padding-left:5px; /* 左侧内边距：5px */
  padding-right:5px; /* 右侧内边距：5px */
}

/* 树视图/表格视图选中禁用项样式 */
QTreeView::item:selected:disabled, QTableView::item:selected:disabled{
  background:rgb(80,80,80); /* 背景色：深灰色 */
}

/* 树视图分支基础样式 */
QTreeView::branch{
  background-color:palette(base); /* 背景色：系统基础色 */
}

/* 树视图分支闭合状态（无兄弟节点） */
QTreeView::branch:has-children:!has-siblings:closed,
QTreeView::branch:closed:has-children:has-siblings{
  border-image:none; /* 无边框图片 */
}

/* 树视图分支打开状态（无兄弟节点） */
QTreeView::branch:open:has-children:!has-siblings,
QTreeView::branch:open:has-children:has-siblings{
  border-image:none; /* 无边框图片 */
}

/* 树视图分支选中/悬停状态 */
QTreeView:branch:selected, QTreeView:branch:hover {
  background: url(":/qss_icons/dark/dark_icons/transparent.png"); /* 背景透明 */
}

/* 树视图分支线样式（有兄弟节点但不相邻） */
QTreeView:branch:has-siblings:!adjoins-item {
  border-image: url(":/qss_icons/dark/dark_icons/branch_line.png") 0; /* 分支线图标 */
}

/* 树视图分支线样式（有兄弟节点且相邻） */
QTreeView:branch:has-siblings:adjoins-item {
  border-image: url(":/qss_icons/dark/dark_icons/branch_more.png") 0; /* 分支更多图标 */
}

/* 树视图分支线样式（无子节点且相邻） */
QTreeView:branch:!has-children:!has-siblings:adjoins-item {
  border-image: url(":/qss_icons/dark/dark_icons/branch_end.png") 0; /* 分支结束图标 */
}

/* 树视图分支闭合状态图标 */
QTreeView:branch:has-children:!has-siblings:closed, 
QTreeView:branch:closed:has-children:has-siblings {
  border-image: none; /* 无边框图片 */
  image: url(":/qss_icons/dark/dark_icons/branch_closed.png"); /* 分支闭合图标 */
}

/* 树视图分支打开状态图标 */
QTreeView:branch:open:has-children:!has-siblings, 
QTreeView:branch:open:has-children:has-siblings {
  border-image: none; /* 无边框图片 */
  image: url(":/qss_icons/dark/dark_icons/branch_open.png"); /* 分支打开图标 */
}

/* 树视图分支闭合悬停状态图标 */
QTreeView:branch:has-children:!has-siblings:closed:hover, 
QTreeView:branch:closed:has-children:has-siblings:hover {
  image: url(":/qss_icons/dark/dark_icons/branch_closed_focus.png"); /* 分支闭合悬停图标 */
}

/* 树视图分支打开悬停状态图标 */
QTreeView:branch:open:has-children:!has-siblings:hover, 
QTreeView:branch:open:has-children:has-siblings:hover {
  image: url(":/qss_icons/dark/dark_icons/branch_open_focus.png"); /* 分支打开悬停图标 */
}

/* 垂直滚动条基础样式 */
QScrollBar:vertical{
  background:palette(base); /* 背景色：系统基础色 */
  border-top-right-radius:1px; /* 右上角圆角：1px */
  border-bottom-right-radius:1px; /* 右下角圆角：1px */
  width:8px; /* 宽度 */
  margin:0px; /* 外边距：0px */
}

/* 垂直滚动条滑块样式 */
QScrollBar::handle:vertical{
  background-color:palette(alternate-base); /* 背景色：系统交替基础色 */
  border-radius:1px; /* 圆角：1px */
  min-height:20px; /* 最小高度：20px */
  margin:1px 2px 1px 2px; /* 外边距：上下1px，左右2px */
}

/* 垂直滚动条滑块悬停状态 */
QScrollBar::handle:vertical:hover{
  background-color:palette(highlight); /* 背景色：系统高亮色 */
}

/* 垂直滚动条增加按钮 */
QScrollBar::add-line:vertical{
  background:none; /* 背景透明 */
  height:0px; /* 高度：0px */
  subcontrol-position:right; /* 子控件位置：右侧 */
  subcontrol-origin:margin; /* 子控件原点：边距 */
}

/* 垂直滚动条减少按钮 */
QScrollBar::sub-line:vertical{
  background:none; /* 背景透明 */
  height:0px; /* 高度：0px */
  subcontrol-position:left; /* 子控件位置：左侧 */
  subcontrol-origin:margin; /* 子控件原点：边距 */
}

/* 水平滚动条基础样式 */
QScrollBar:horizontal{
  background:palette(base); /* 背景色：系统基础色 */
  height:8px; /* 高度 */
  margin:0px; /* 外边距：0px */
}

/* 水平滚动条滑块样式 */
QScrollBar::handle:horizontal{
  background-color:palette(alternate-base); /* 背景色：系统交替基础色 */
  border-radius:1px; /* 圆角：1px */
  min-width:20px; /* 最小宽度：20px */
  margin:2px 1px 2px 1px; /* 外边距：上下2px，左右1px */
}

/* 水平滚动条滑块悬停状态 */
QScrollBar::handle:horizontal:hover{
  background-color:palette(highlight); /* 背景色：系统高亮色 */
}

/* 水平滚动条增加按钮 */
QScrollBar::add-line:horizontal{
  background:none; /* 背景透明 */
  width:0px; /* 宽度：0px */
  subcontrol-position:bottom; /* 子控件位置：底部 */
  subcontrol-origin:margin; /* 子控件原点：边距 */
}

/* 水平滚动条减少按钮 */
QScrollBar::sub-line:horizontal{
  background:none; /* 背景透明 */
  width:0px; /* 宽度：0px */
  subcontrol-position:top; /* 子控件位置：顶部 */
  subcontrol-origin:margin; /* 子控件原点：边距 */
}

/* 水平滑块手柄样式 */
QSlider::handle:horizontal{
  border-radius:4px; /* 圆角：4px */
  border:1px solid rgba(25,25,25,255); /* 边框：1px深灰色 */
  background-color:palette(alternate-base); /* 背景色：系统交替基础色 */
  min-height:20px; /* 最小高度：20px */
  margin:0 -4px; /* 外边距：左右-4px（扩展手柄点击区域） */
}

/* 水平滑块手柄悬停状态 */
QSlider::handle:horizontal:hover{
  background:palette(highlight); /* 背景色：系统高亮色 */
}

/* 水平滑块已滑过区域 */
QSlider::add-page:horizontal{
  background:palette(base); /* 背景色：系统基础色 */
}

/* 水平滑块未滑过区域 */
QSlider::sub-page:horizontal{
  background:palette(highlight); /* 背景色：系统高亮色 */
}

/* 水平滑块未滑过区域（禁用状态） */
QSlider::sub-page:horizontal:disabled{
  background:rgb(80,80,80); /* 背景色：深灰色 */
}